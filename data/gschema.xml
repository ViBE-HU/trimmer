<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/com/github/phoneybadger/trimmer/" id="com.github.phoneybadger.trimmer">
    <key name="position" type="(ii)">
      <default>(-1,-1)</default>
      <summary>Window position</summary>
      <description>The last saved window position (x,y)</description>
    </key>
    <key name="dimensions" type="(ii)">
      <default>(640,480)</default>
      <summary>Window dimensions</summary>
      <description>The last saved dimensions of the window (width, height)</description>
    </key>
    <key name="maximized" type="b">
      <default>false</default>
      <summary>Maximized</summary>
      <description>Whether the window was maximized</description>
    </key>
  </schema>
</schemalist>
